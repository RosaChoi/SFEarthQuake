<!-- Earthquake Update Report Dashboard -->
  <div class="container dashboard">
    <div class="row one">
      <span id="displayDate"><%= @time %></span>
    </div>
    <div class="row two">
      <div class="col-lg-7 col-sm-5 col-xs-8 geo-coordinates">
        <span id="displayLocation"><%= @location %></span>
      </div>
      <div class="col-lg-4 col-sm-3 col-xs-8 magnitude">
        <span id="displayMagnitude"><%= @magnitude %>m</span>
      </div>
    </div>
    <div class="row three">
      <div class="col-lg-12 col-sx-11 sf-report-title">
        <h1>SAN FRANCISCO BAY AREA</h1>
        <p class="report">Earthquake Update Report | Last Earthquake Greater Than 6.0</p>
      </div>
    </div>
    <div class="row four">
      <aside class="col-lg-4 to-do">
        <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Beatae, ratione, fuga, suscipit, assumenda...</p>
        <a href="http://www.redcrossstore.org/item/321406">
          <img src="/img/redcrossearthquakekit.jpg" alt="Red Cross earthquake kit">
          <p>Buy an earthquake preparedness kit<p>
        </a>
      </aside>
      <div class="col-lg-7 probability">
        <div id="displayProbability" data-probability="<%= @probability %>"></div>
      </div>
    </div>
    <div class="row five">

    </div>